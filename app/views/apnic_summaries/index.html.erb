<h1>Listing apnic_summaries</h1>

<table>
  <tr>
    <th>Registry</th>
    <th>Type</th>
    <th>Count</th>
    <th>Summary</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @apnic_summaries.each do |apnic_summary| %>
  <tr>
    <td><%= apnic_summary.registry %></td>
    <td><%= apnic_summary.type %></td>
    <td><%= apnic_summary.count %></td>
    <td><%= apnic_summary.summary %></td>
    <td><%= link_to 'Show', apnic_summary %></td>
    <td><%= link_to 'Edit', edit_apnic_summary_path(apnic_summary) %></td>
    <td><%= link_to 'Destroy', apnic_summary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

